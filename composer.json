{
	"name": "coldtrick/poll",
	"description": "Adds basic poll functionality to your Elgg website",
	"homepage": "https://www.coldtrick.com",
	"type": "elgg-plugin",
	"keywords": ["elgg", "plugin", "poll"],
	"license": "GPL-2.0-only",
	"support": {
		"source": "https://github.com/ColdTrick/poll",
		"issues": "https://github.com/ColdTrick/poll/issues"
	},
	"require": {
		"composer/installers": "^1.0.8",
		"npm-asset/chart.js": "~4.4.0"
	},
	"conflict": {
		"elgg/elgg": "<5.1"
	},
	"repositories": [
		{
            "type": "composer",
            "url": "https://asset-packagist.org"
		}
	],
	"config": {
		"fxp-asset": {
            "enabled": false
		},
		"allow-plugins": {
			"composer/installers": true
		}
	}
}
