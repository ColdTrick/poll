{
	"name": "coldtrick/poll",
	"description": "Adds basic poll functionality to your Elgg website",
	"type": "elgg-plugin",
	"keywords": ["elgg", "plugin", "poll"],
	"license": "GPL-2.0-only",
	"support": {
		"source": "https://github.com/ColdTrick/poll",
		"issues": "https://github.com/ColdTrick/poll/issues"
	},
	"require": {
		"composer/installers": "^1.0.8",
		"npm-asset/chart.js": "~2.7.3"
	},
	"require-dev": {
		"coldtrick/sniffs": "dev-master",
		"coldtrick/releases": "dev-master"
	},
	"conflict": {
		"elgg/elgg": "<3.3"
	},
	"repositories": [
		{
            "type": "composer",
            "url": "https://asset-packagist.org"
		}
	],
	"config": {
		"fxp-asset": {
            "enabled": false
		}
	}
}
